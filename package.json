{
  "name": "jcamp-data-test",
  "version": "0.7.0",
  "description": "provides JCAMP-DX NMR data files for testing purposes",
  "keywords": [
    "magnetic resonance",
    "nmr",
    "JCAMP",
    "dataset"
  ],
  "main": "lib/index.js",
  "module": "src/index.js",
  "types": "jcamp-data-test.d.ts",
  "files": [
    "lib",
    "src",
    "data/*",
    "jcamp-data-test.d.ts"
  ],
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "rollup -c",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "jest --coverage"
  },
  "author": "Julien Wist",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "eslint": "^8.56.0",
    "eslint-config-cheminfo": "^9.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rollup": "^4.10.0"
  },
  "dependencies": {
    "file-collection": "^0.2.0",
    "jcampconverter": "^9.6.1",
    "ml-spectra-processing": "^12.10.2"
  }
}
