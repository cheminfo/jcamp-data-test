{
  "name": "jcamp-data-test",
  "version": "0.1.0",
  "description": "provides JCAMP-DX NMR data files for testing purposes",
  "keywords": [
    "magnetic resonance",
    "nmr",
    "JCAMP",
    "dataset"
  ],
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src",
    "data/*"
  ],
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "rollup -c",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "jest --coverage"
  },
  "author": "Julien Wist",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "eslint": "^8.15.0",
    "eslint-config-cheminfo": "^8.0.2",
    "jest": "^28.1.3",
    "prettier": "^2.6.2",
    "rollup": "^2.72.1"
  },
  "dependencies": {
    "filelist-utils": "^0.5.0"
  }
}